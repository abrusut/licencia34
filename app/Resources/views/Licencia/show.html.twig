{% extends "AppBundle:Page:index.html.twig" %}

{% block nuevoybarradebusquedas %}
{% endblock %}
{% block styles %}
     <!-- Le styles -->
        <link href="{{ asset('bundles/petkoparacrudgenerator/css/bootstrap.min.css') }}" rel="stylesheet"> 
         <link href="{{ asset('css/ajustesPetkoparaStg.css') }}" rel="stylesheet">              
{% endblock %}

{% block javascriptsIncludes %} 
        <!-- Le HTML5 shim, for IE6-8 support of HTML5 elements -->
        <!--[if lt IE 9]>
        <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
        <![endif]-->
    <script src="{{ asset('bundles/petkoparacrudgenerator/js/jquery.min.js') }}"></script>
    <script src="{{ asset('bundles/petkoparacrudgenerator/js/bootstrap.min.js') }}"></script>
    <script src="{{ asset('bundles/petkoparacrudgenerator/js/petkopara-crud-generator.js') }}"></script>
      <!-- Load jQuery UI Main JS (datepicker) -->
    <script src="http://code.jquery.com/ui/1.10.2/jquery-ui.min.js"></script>  
{% endblock %} 

{% block title %}
{{ parent() }} - comprobante 
{% endblock %}

{% block cuerpo %}
{% block body %}

<div class="row">
    <div class="page-header">
        <h2><a href="{{ path('licencia') }}" >Ver licencia <span class="glyphicon glyphicon-eye-open" aria-hidden="true"></span> </a></h2>
    </div>
</div>

<div class="row">
    <div class="col-md-6">
        <p><strong>Id</strong></p>
        <p>
            {{ licencium.id }}
        </p>

    </div>
     <div class="col-md-6">
        <p><strong>Persona</strong></p>
        <p>
            {{ licencium.persona.nombreCompleto}}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Tipo Licencia</strong></p>
        <p>
            {{ licencium.tipoLicencia }}
        </p>

    </div>
     <div class="col-md-6">
        <p><strong>Pagado</strong></p>
        <p>
            {% if licencium.comprobante.fechaCobro %}{{ licencium.comprobante.fechaCobro|date('Y-m-d H:i:s') }}{% endif %} 
        </p>

    </div>
    
    <div class="col-md-6">
        <p><strong>Fecha Emitida</strong></p>
        <p>
            {% if licencium.fechaEmitida %}{{ licencium.fechaEmitida|date('Y-m-d H:i:s') }}{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Fecha Desde</strong></p>
        <p>
            {% if licencium.fechaDesde %}{{ licencium.fechaDesde|date('Y-m-d H:i:s') }}{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Fecha Vencimiento</strong></p>
        <p>
            {% if licencium.fechaVencimiento %}{{ licencium.fechaVencimiento|date('Y-m-d H:i:s') }}{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Numero</strong></p>
        <p>
            {{ licencium.numero }}
        </p>

    </div></div>

<hr/>


<div class="form-group">
    <a class="btn btn-default" href="{{ path('licencia') }}">
        <span class="glyphicon glyphicon-list" aria-hidden="true"></span>
        Volver a Listado
    </a>
    <a class="btn btn-primary" href="{{ path('licencia_edit', { 'id':  licencium.id }) }}">
        <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
        Editar
    </a>    
</div>

</div>


{% endblock %}
{% endblock %}

